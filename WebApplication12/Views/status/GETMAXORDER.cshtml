
@{
    ViewData["Title"] = "GETMAXORDER";
}
@model IEnumerable<WebApplication12.BL.VM.GETMAXORDER>
@section style{
    <link href="~//assets/css/StyleSheet.css" rel="stylesheet" />
    <link href="../assets/css/Customer.css" rel="stylesheet" />
       <link rel="stylesheet" href="~/assets/css/Customer.css" />
<link rel="stylesheet" href="~/assets/css/status.css" />
}
<center>
    <h1 style="margin-top:100px;"> Max Order Details</h1>
</center>
<table class="table table-bordered table-hover table-striped x">
<thead>
    <tr>
        <th>Customer Name</th>
         <th>Customer mail</th>
          <th>order Number</th>
          <th>Price</th>

    </tr>
</thead>
<tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.customername</td>
                   <td>@item.customerMAil</td>
                      <td>@item.ordernum</td>
                         <td>@item.totalPrice</td>
            </tr>
        }
    </tbody>
</table>
<div >
        <a class="btn btn-pill btn-primary y" asp-controller="status" asp-action="Index">Back</a>

</div>







